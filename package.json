    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "daisyui": "^3.6.3",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "lexical": "^0.12.0",
    "next": "^13.4.19",
    "postcss": "8.4.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6"
